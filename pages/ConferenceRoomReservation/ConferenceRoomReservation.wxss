/* ----------页面顶部区域------------ */
.content {
  height: 2125rpx;
  border: none;

}

.content-body1 {
  height: 242rpx;

}

.backindex {
  display: flex;
  height: 60rpx;
  border-bottom: 1px solid gray;
}

.backindex image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 8rpx
}

.backindex view {
  line-height: 60rpx;
  margin-left: 40rpx;
  font-weight: bold;
  font-size: 36rpx
}

/* 会议室 */
.cb1-content {
  width: 100%;
  height: 160rpx;
  display: flex;
  margin-top: 20rpx;
  border-bottom: 1px solid #6C8998
}

.cb1-content image {
  width: 200rpx;
  height: 140rpx;
  margin-left: 20rpx
}

.cb1-right {
  width: 80%;
  height: 140rpx;
  margin-left: 20rpx
}

.cb1-right :nth-child(1) {
  font-size: 34rpx;
  margin-top: 20rpx;
  font-weight: bolder
}

.cb1-right :nth-child(2) {
  font-size: 30rpx;
  margin-top: 10rpx;
  color: #6C8998
}

